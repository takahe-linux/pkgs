#!/usr/bin/sh

set -e
finish() { echo "Success!"; exit 0; }
fail() { printf "%s\n" "$1"; echo "Fail!"; exit 1; }

# Test busybox.
echo "busybox:"
busybox ls
busybox true || \
    fail "'busybox true' failed"
busybox false && \
    fail "'busybox false' failed"

# Check that pacman runs.
echo "pacman -Qq output:"
pacman -Qq

finish

    'binutils'
    'bison'
    'fakeroot'
    'flex'
    'gcc'
    'linux-api-headers'
    'm4'
    'make'
    'musl'
    'pkg-config'
    'xz'

    # base:
    'busybox'
    'coreutils'
    'util-linux'
    'findutils'
    'bash'
    'file'
    'filesystem'
    'gettext'
    'pacman'

    # Required for booting:
    'initscripts'
    'backhand'
    'mdev'
    'sinit'
    'linux'
    )


# Package information
pkgname='packages'
pkgver='1.0'
pkgrel=33
pkgdesc="Meta-package for the cross-compiled packages"

# Additional information...
arch=('any')

# Dependency information.
# We basically require all of base, base-devel, and a kernel.
depends=(
    # base-devel:
    'binutils'
    'bison'
    'fakefakeroot'
    'flex'
    'gcc'
    'm4'
    'make'
    'musl'
    'xz'

    # base:
    'busybox'
    'coreutils'
    'util-linux'
    'findutils'
    'bash'
    'file'
    'filesystem'
    'gettext'
    'pacman'
    # Required for booting:
    'initscripts'
    'mdev'
    'sinit'

    # kernel:
    'linux'
    )

package() {
    :
}
